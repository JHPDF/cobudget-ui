### @ngInject ###
module.exports = (UserModel) ->
  class AllocationModel
    constructor: (data = {}) ->
      @id = data.id
      @userId = data.userId
      @roundId = data.roundId
      @amountCents = data.amountCents

    serialize: ->
      {
        id: @id,
        userId: @userId
        roundId: @roundId
        amountCents: @amountCents
      }