debug = require('debug')
# ridiculous hack for @ngInject due to code at top from coffee class extension
null
### @ngInject ###
module.exports = (RestfulStore, MembershipModel) ->
  class MembershipsStore extends RestfulStore
    path: (params = {}, op) ->
      debug("path", params, op)
      if op == "all"
        "#{@baseUrl}/groups/#{params.groupId}/#{@plural}"
      else
        super(params, op)

  new MembershipsStore(
    singular: "membership"
    plural: "memberships"
    Model: MembershipModel
  )