### @ngInject ###
module.exports = (BaseModel) ->
  BaseModel.extend
    
    type: "FixedCost"

    props:
      roundId: "number"
      name: "string"
      description: "string"
      amount: "decimal"

    serialize: ->
      serialized = BaseModel.prototype.serialize.call(@)
      serialized.amount = serialized.amount.toFixed(2)
      serialized
