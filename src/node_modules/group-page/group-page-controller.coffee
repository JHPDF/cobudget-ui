module.exports = ($rootScope, $scope, $stateParams, group) ->
    $rootScope.groupId = $stateParams.groupId
    $scope.group = group

    $scope.banner = {
      name: "group"
      parent: {
        name: group.name
        ref: "layout.group.rounds({ groupId: group.id })"
        logo: "img/logo-enspiral.png"
      }
    }

    $scope.tabs = [{
      name: "rounds"
      label: "ROUNDS"
    }, {
      name: "members"
      label: "MEMBERS"
    }]
