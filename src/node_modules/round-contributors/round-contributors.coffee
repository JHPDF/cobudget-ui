### @ngInject ###
module.exports = {
  url: '/contributors'
  template: require('./round-contributors-template.html')
  controller: require('./round-contributors-controller.coffee')
  resolve:
    memberships: ($stateParams, MembershipsStore) ->
      MembershipsStore.all(groupId: $stateParams.groupId)
    users: (UsersStore) ->
      UsersStore.all()
}
