### @ngInject ###
module.exports = {
  url: ''
  template: require('./layout-template.html')
  controller: require('./layout-controller.coffee')
  resolve:
    groups: (GroupsStore, AuthService) ->
      user = AuthService.getCurrentUser()
      if user
        GroupsStore.all(memberId: user.id)
      else
        GroupsStore.all()
}
