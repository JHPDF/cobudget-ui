debug = require('debug')
recase = require('recase').create({})
# ridiculous hack for @ngInject due to code at top from coffee class extension
null
### @ngInject ###
module.exports = (RestfulStore, FixedCostModel) ->
  class  FixedCostsStore extends RestfulStore
    path: (params = {}, op) ->
      debug("path", params, op)
      if op == "all"
        plural = recase.snakify(@plural)
        "#{@baseUrl}/rounds/#{params.roundId}/#{plural}"
      else
        super(params, op)

  new FixedCostsStore(
    singular: "fixedCost"
    plural: "fixedCosts"
    Model: FixedCostModel
  )