### @ngInject ###
module.exports = {
  abstract: true
  url: '/groups/:groupId/rounds/:roundId'
  template: require('./round-page-template.html')
  controller: require('./round-page-controller.coffee')
  resolve:
    group: (GroupsStore, $stateParams) ->
      GroupsStore.get($stateParams.groupId)
    round: (RoundsStore, $stateParams) ->
      RoundsStore.get($stateParams.roundId)
}
