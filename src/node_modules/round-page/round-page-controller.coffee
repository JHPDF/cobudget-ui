module.exports = ($scope, group) ->
    $scope.group = group
    $scope.round = group.latestRound

    $scope.tabs = [{
      name: "overview"
      label: "OVERVIEW"
    }, {
      name: "buckets"
      label: "BUCKETS"
    }, {
      name: "contributors"
      label: "CONTRIBUTORS"
    }]


    $scope.banner = {
      name: "round"
      parent: {
        name: group.name
        ref: "layout.group.rounds({ groupId: group.id })"
        logo: "img/logo-enspiral.png"
      }
      child: {
        name: $scope.round.name
        ref: ".overview"
      }
    }
